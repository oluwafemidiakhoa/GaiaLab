{
  "name": "gaialab-chatgpt-app",
  "version": "0.1.0",
  "description": "AI-Powered Biological Intelligence Platform - Transform gene lists into publication-quality insights in 60 seconds",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --watch public src/server.js",
    "eval": "node scripts/gaialab-eval.js",
    "eval:gold": "node scripts/gaialab-eval.js --benchmarks data/benchmarks/gold-standard.json --out data/eval/gold-last-run.json --bypassCache true",
    "eval:dashboard": "node scripts/gaialab-dashboard.js --in data/eval/last-run.json --out data/eval/dashboard.html"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@slack/bolt": "^4.6.0",
    "3d-force-graph": "^1.73.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "ngl": "^2.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.10.0",
    "simple-statistics": "^7.8.8",
    "three": "^0.160.0",
    "xml2js": "^0.6.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
